config interface loopback
    option ifname   lo
    option proto    static
    option ipaddr   127.0.0.1
    option netmask  255.0.0.0

config interface lan
    option ifname   wlan0
    option type     bridge
    option proto    static
    option ipaddr   192.168.22.1
    option netmask  255.255.254.0

config interface wan
    option ifname   eth0
    option proto    dhcp
