config wifi-device  radio0
    option type     mac80211
    option channel  11
    option macaddr  00:03:7f:c0:7a:03
    option hwmode   11ng
    option htmode   HT20
    list ht_capab   SHORT-GI-40
    list ht_capab   DSSS_CCK-40

config wifi-iface
    option device   radio0
    option network  lan
    option mode     ap
    option ssid     wrt160nl
    option encryption none

